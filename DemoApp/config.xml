<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mopinion.mopinionsdk.demoapp" version="0.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DemoApp</name>
    <description>
        A sample Apache Cordova application using the MopinionSDK plugin.
    </description>
    <author href="http://mopinion.com">
        Mopinion BV
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="SwiftVersion" value="5" />
    </platform>
</widget>
